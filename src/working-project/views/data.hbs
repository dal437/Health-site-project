	<body>
		<h1><b><center>Data Information</center><b></h1>
    <style>
    table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
			align: center;
    }
    </style>
    <table>
    <tr>
      <th>Age</th>
      <th>Gender</th>
      <th>State</th>
			<th>Ethnicity</th>
    </tr>
		<tbody>
	    {{#each infos as |info|}}
	    <tr>
	    	<td>{{info.age}}</td>
	    	<td>{{info.gender}}</td>
	    	<td>{{info.state}}</td>
				<td>{{info.ethnicity}}</td>
	  	</tr>
	  	{{/each}}
	</tbody>
</table>
